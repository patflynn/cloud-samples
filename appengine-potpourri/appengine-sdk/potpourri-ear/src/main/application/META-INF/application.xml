<?xml version="1.0" encoding="UTF-8"?>
<application
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/application_5.xsd"
        version="5">

    <description>App Engine Multi-module EAR based sample</description>
    <display-name>Potpourri EAR</display-name>

    <module>
        <web>
            <web-uri>endpoints-frontend-1.0</web-uri>
            <context-root>endpoints</context-root>
        </web>
    </module>
    <module>
        <web>
            <web-uri>jdo-backend-1.0</web-uri>
            <context-root>jdo</context-root>
        </web>
    </module>
</application>