<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>${appengineProjectName}</application>
  <threadsafe>true</threadsafe>
  <version>1</version>
  <static-files>
    <include path="/index.html"/>
    <include path="/error/**"/>
  </static-files>
  <static-error-handlers>
    <handler error-code="default" file="/error/default.html"/>
  </static-error-handlers>
</appengine-web-app>